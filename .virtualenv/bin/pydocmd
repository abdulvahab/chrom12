#!/Users/wizofe/PycharmProjects/chrom12/.virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pydoc-markdown==2.0.3','console_scripts','pydocmd'
__requires__ = 'pydoc-markdown==2.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pydoc-markdown==2.0.3', 'console_scripts', 'pydocmd')()
    )
